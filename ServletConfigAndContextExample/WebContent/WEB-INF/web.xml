<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>ServletConfigAndContextExample</display-name>
  
  
  <servlet>
  	<servlet-name>abc</servlet-name>
  	<servlet-class>com.khaplu.MyServlet</servlet-class>
  	<init-param>
  		<param-name>lastName</param-name>
  		<param-value>Hameed</param-value>
  	</init-param>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>abc</servlet-name>
  	<url-pattern>/home</url-pattern>
  </servlet-mapping>
  
  <context-param>
  	<param-name>name</param-name>
  	<param-value>Waqas</param-value>
  </context-param>
  <context-param>
  	<param-name>sport</param-name>
  	<param-value>Football</param-value>
  </context-param>
</web-app>